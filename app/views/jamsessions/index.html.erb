<div class="index-container">
  <div class="row">
    <div class="col-6 offset-3">

    <div id="map"
      style="width: 100%; height: 600px;"
      data-markers="<%= @markers.to_json %>"
      data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
     </div>

      <div class="card-jamsessions">
        <div class="card-jamsession-infos">
          <% @jamsessions.each do |jamsession| %>
            <%= render "card", jamsession: jamsession %>
          <% end %>
        </div>  
      </div>
    </div>
  </div>
</div>

